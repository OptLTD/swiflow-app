{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "protocol": "stdio"
    },
    "mcp-midscene": {
      "command": "npx",
      "args": [
        "-y",
        "@midscene/mcp"
      ],
      "env": {
        "OPENAI_API_KEY": "{{API_KEY@string::Your LLM Api Access Key, e.g.: sk....}}",
        "OPENAI_BASE_URL": "{{BASE_URL@string::Your LLM Api Base Url, e.g.: https://openrouter.ai/api/v1}}",
        "MIDSCENE_MODEL_NAME": "{{MODEL_NAME@string::Your LLM Api Model Name, e.g.: gpt-4o}}",
        "MCP_SERVER_REQUEST_TIMEOUT": "{{TIMEOUT@number::Connect midscene timeout, e.g.: 800000}}"
      }
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/tmp/powerlog"
      ]
    },
    "mysql": {
      "command": "npx",
      "args": [
        "-y",
        "@benborla29/mcp-server-mysql"
      ],
      "env": {
        "MYSQL_HOST": "127.0.0.1",
        "MYSQL_PORT": "3306",
        "MYSQL_USER": "root",
        "MYSQL_PASS": "123456",
        "MYSQL_DB": "demo",
        "ALLOW_INSERT_OPERATION": "false",
        "ALLOW_UPDATE_OPERATION": "false",
        "ALLOW_DELETE_OPERATION": "false"
      }
    }
  }
}